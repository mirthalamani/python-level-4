print("SUM OF N NUMBER")
print("---------------")

sn = int(input("Enter the starting number: "))
en = int(input("Enter the ending number: "))

print("Result")
print("------")
print("Series:")

count = 0
total_sum = 0

for i in range(sn, en + 1):
    if (i % 5 == 0) and (i % 3 == 0):
        print("+" + str(i), end=" ")
        total_sum += i
        count += 1

print("Sum value:", total_sum)
print("Count value:", count)
